#include <stdio.h>

int main() {
    int n, count = 0;

    // Введення числа
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);

    // Перебір можливих дільників
    for (int m = 1; m < n; m++) {
        // Перевірка умови рівного дільника
        if (n / m == n % m) {
            count++;
        }
    }

    // Виведення результату
    printf("Кількість рівних дільників числа %d: %d\n", n, count);

    return 0;
}
